{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "esModuleInterop": false,
    "jsx": "react",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "noEmit": true,
    "paths": {
      "@/hooks/*": ["src/hooks/*"]
    },
    "skipLibCheck": false,
    "target": "ESNext"
  },
  "include": ["./src"]
}
