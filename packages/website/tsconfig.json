{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "jsx": "preserve",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "noEmit": true,
    "paths": {
      "@coderscamp/ui/*": ["../ui/src/*"],
      "@/assets/*": ["src/assets/*"],
      "@/components/*": ["src/components/*"],
      "@/constants": ["src/constants"]
    },
    "target": "es5"
  },
  "include": ["next-env.d.ts", "declarations.d.ts", "src", "../ui/src"],
  "exclude": ["node_modules"]
}
