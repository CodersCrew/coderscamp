datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  githubId Int    @unique
  fullName String
  email    String
  image    String
}

model LearningMaterial {
  id             Int    @id @default(autoincrement())
  url            String
  completedCount Int    @default(0)
  userId         Int    @unique
}
