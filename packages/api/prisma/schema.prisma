datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id              String  @id
  githubId        Int     @unique
  fullName        String?
  email           String?
  image           String
  town            String?
  gender          String?
  birthYear       Int?
  educationStatus String?
}
