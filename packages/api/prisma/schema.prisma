datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String @id @default(uuid())
  githubId Int    @unique
  fullName String
  email    String
  image    String
}

model LearningMaterial {
  id             String @id @default(uuid())
  url            String
  completedCount Int    @default(0)
  userId         String @unique
}
