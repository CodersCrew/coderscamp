{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "incremental": true,
    "lib": ["ESNext"],
    "module": "commonjs",
    "outDir": "./dist",
    "paths": {
      "@coderscamp/shared/*": ["../shared/src/*"],
      "@/common/*": ["src/common/*"],
      "@/auth/*": ["src/auth/*"],
      "@/users/*": ["src/users/*"],
      "@/prisma/*": ["src/prisma/*"],
      "@/automation/*": ["src/module/automation/*"],
      "@/read/*": ["src/module/read/*"],
      "@/write/*": ["src/module/write/*"],
      "@/module/*": ["src/module/shared/*"],
      "@/events/*": ["src/module/shared/events/*"],
      "@/commands/*": ["src/module/shared/commands/*"],
      "@/crud/*": ["src/crud/*"]
    },
    "sourceMap": true,
    "target": "es2017"
  },
  "include": ["src", "../shared/src"]
}
